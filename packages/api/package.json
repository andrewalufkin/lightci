{
	"name": "@lightci/api",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": "20.x"
	},
	"scripts": {
		"start": "node dist/index.js",
		"dev": "tsx watch src/server.ts",
		"build": "tsc",
		"test": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js --forceExit",
		"test:force-exit": "FORCE_EXIT_TESTS=true NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js --forceExit",
		"lint": "eslint src/**/*.ts",
		"proto": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/proto --ts_proto_opt=esModuleInterop=true,outputServices=grpc-js,env=node proto/engine.proto",
		"apply-migration": "node apply-migration.js",
		"drop-constraint": "npx prisma db execute --file ./prisma/drop-constraint.sql"
	},
	"dependencies": {
		"@aws-sdk/client-ec2": "^3.756.0",
		"@grpc/grpc-js": "^1.9.13",
		"@grpc/proto-loader": "^0.7.10",
		"@octokit/rest": "^21.1.1",
		"@prisma/client": "^6.5.0",
		"@types/bcrypt": "^5.0.2",
		"@types/bull": "^4.10.4",
		"@types/mime-types": "^2.1.4",
		"@types/node-cron": "^3.0.11",
		"ajv": "^8.12.0",
		"ajv-formats": "^2.1.1",
		"axios": "^1.7.9",
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"bull": "^4.16.5",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-validator": "^7.0.1",
		"glob": "^10.3.10",
		"jsonwebtoken": "^9.0.2",
		"mime-types": "^2.1.35",
		"node-cron": "^3.0.3",
		"prisma": "^6.5.0",
		"ts-proto": "^1.165.1",
		"uuid": "^9.0.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@babel/core": "^7.26.9",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-typescript": "^7.26.0",
		"@jest/globals": "^29.7.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/express-serve-static-core": "^5.0.6",
		"@types/glob": "^8.1.0",
		"@types/jest": "^29.5.11",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.10.5",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"babel-jest": "^29.7.0",
		"eslint": "^8.56.0",
		"jest": "^29.7.0",
		"jest-mock-extended": "^4.0.0-beta1",
		"nodemon": "^3.0.2",
		"supertest": "^6.3.4",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.2",
		"typescript": "^5.8.2"
	},
	"main": "index.js",
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"jest": {
		"preset": "ts-jest/presets/default-esm",
		"testEnvironment": "node",
		"setupFilesAfterEnv": [
			"./test/setup.ts"
		],
		"transform": {
			"^.+\\.tsx?$": [
				"ts-jest",
				{
					"useESM": true,
					"tsconfig": "tsconfig.json"
				}
			],
			"^.+\\.jsx?$": [
				"babel-jest",
				{
					"presets": [
						[
							"@babel/preset-env",
							{
								"targets": {
									"node": "current"
								}
							}
						]
					]
				}
			]
		},
		"transformIgnorePatterns": [
			"node_modules/(?!(@octokit|@smithy|@aws-sdk|@babel/runtime|universal-user-agent|before-after-hook|@octokit/.*|node-fetch)/)"
		],
		"moduleFileExtensions": [
			"js",
			"mjs",
			"cjs",
			"jsx",
			"ts",
			"tsx",
			"json",
			"node"
		],
		"extensionsToTreatAsEsm": [
			".ts",
			".tsx"
		],
		"moduleNameMapper": {
			"^(\\.{1,2}/.*)\\.js$": "$1"
		}
	}
}
